#include <stdio.h>
#include <stdlib.h>
#define n 3

//Estructura, dato del problema
struct TipoPunto{
	double abscisa;
	double ordenada;};
struct TipoCiudad{
	struct TipoPunto situacion;
	char nombre[50];
};
int main(){
	//ingreso las ciudades con su determinadas coordenadas
	struct TipoCiudad origen[n];
	int i=0;
	while(i<n){
		printf("ingrese una ciudad\n");
		scanf("%s", origen[i].nombre);
		printf("%s", origen[i].nombre);
		i++;
	}
}
